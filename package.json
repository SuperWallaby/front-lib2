{
  "name": "@janda-com/front",
  "version": "2.1.6",
  "description": "@jd-com/front",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "jsnext:main": "build/index.es.js",
  "scripts": {
    "prebuild": "rm -rf ./build",
    "buildw": "rollup -c -w",
    "build": "rollup -c",
    "onlyBuild": "rollup -c",
    "scssBuild": "cd ./src/scss && npx copy **/**/* **/* * ../../build/scss && cd ../../",
    "postbuild": "cd ./src/scss && npx copy **/**/* **/* * ../../build/scss && cd ../../ && yarn publish",
    "postonlyBuild": "cd ./src/scss && npx copy **/**/* **/* * ../../build/scss && cd ../../",
    "lint": "standard",
    "format": "prettier-standard --format",
    "test": "jest --coverage",
    "sb": "start-storybook --port 9091",
    "storybook": "start-storybook --port 9091",
    "s-build": "build-storybook",
    "s-upload": "aws s3 cp storybook-static s3://stayjanda.modules.public/jdUi/2.0.25/  --recursive",
    "s-b-u": "yarn s-build && yarn s-upload",
    "precodegen": "apollo schema:download --endpoint=https://temp-booking.stayjanda.cloud/graphql",
    "codegen": "apollo codegen:generate src/types/api.d.ts --queries=src/apollo/queries.ts --addTypename --localSchemaFile=schema.json --target=typescript --outputFlat"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@rollup/plugin-json": "^4.0.2",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.14",
    "@storybook/addon-notes": "^5.3.14",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/preset-typescript": "^2.1.0",
    "@storybook/react": "^5.3.14",
    "@storybook/theming": "^5.3.19",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.0.24",
    "@types/jquery": "^3.3.33",
    "@types/leven": "^2.1.2",
    "@types/moment-timezone": "^0.5.12",
    "@types/randomcolor": "^0.5.5",
    "@types/react": "^16.9.17",
    "@types/react-window-size": "^1.2.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "jest": "^24.9.0",
    "leven": "^3.1.0",
    "node-sass": "^7.0.0",
    "react": "^16.12.0",
    "react-day-picker": "^7.4.8",
    "react-dom": "^16.12.0",
    "rollup": "^1.27.13",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "sass-loader": "^8.0.2",
    "standard": "^14.3.1",
    "standard-prettier": "^1.0.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "node-sass": "^4.13.1",
    "react": "^16",
    "react-day-picker": "^7.4.8",
    "react-dom": "^16"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "build/"
    ]
  },
  "standard": {
    "ignore": [
      "node_modules/",
      "build/",
      ".storybook/"
    ],
    "globals": [
      "describe",
      "it",
      "test",
      "expect",
      "afterAll",
      "jest"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@apollo/react-common": "^3.1.3",
    "@types/graphql": "^14.5.0",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^6.0.0",
    "@types/react-modal": "^3.10.5",
    "@types/react-paginate": "^6.2.1",
    "@types/react-select": "^3.0.10",
    "@types/react-slick": "^0.23.4",
    "@types/react-tabs": "^2.3.1",
    "@types/react-toastify": "^4.1.0",
    "apollo-client": "^2.6.8",
    "classnames": "^2.2.6",
    "is-mobile": "^2.2.1",
    "jquery": "^3.4.1",
    "moment-timezone": "^0.5.28",
    "randomcolor": "^0.5.4",
    "react-color": "^2.18.1",
    "react-docgen-typescript-loader": "^3.7.1",
    "react-helmet": "^6.1.0",
    "react-image-file-resizer": "^0.2.1",
    "react-modal": "^3.11.2",
    "react-paginate": "^6.3.2",
    "react-slick": "^0.25.2",
    "react-spring": "^8.0.27",
    "react-tabs": "^3.1.0",
    "react-toastify": "^5.5.0",
    "react-tooltip": "^4.2.6",
    "react-window-size": "^1.2.2"
  }
}
